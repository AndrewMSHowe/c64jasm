{
    "comments": {
        "lineComment": ";",
    },
    "brackets": [
        ["{", "}"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["(", ")"],
        { "open": "'", "close": "'", "notIn": ["string", "comment"] }
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["(", ")"],
    ],
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
}
